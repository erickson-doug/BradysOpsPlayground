{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**/*.csproj"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**",
            "**/*.Tests.csproj"
          ],
          "cwd": "src"
        }
      ],
      "dest": "obj/api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.yml",
          "**/*.md"
        ],
        "src": "api",
        "dest": "api"
      },
      {
        "files": [
          "*.md",
          "toc.yml"
        ]
      }
    ],
    "globalMetadata": {
      "breadcrumb_path": "/azure-docfx/toc.json"
    },
    "dest": "_site",
    "template": [
      "docs.html"
    ]
  }
}