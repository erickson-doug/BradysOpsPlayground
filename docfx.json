{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "swagger/*.json",
            "*.md"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**",
            "output/**"
          ]
        }
      ],
      "dest": "output"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.md"
        ],
        "exclude": [
          "**/obj/**",
          "output/**",
          "**/includes/**"
        ]
      },
      {
        "files": [
          "**/*.yml",
          "**/*.md"
        ],
        "src": "api",
        "dest": "api",
        "exclude": [
          "**/obj/**",
          "output/**",
          "**/includes/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "azure/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {},
    "fileMetadata": {},
    "template": [
      "docs.html"
    ],
    "dest": "output"
  }
}